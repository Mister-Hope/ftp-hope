{
  "name": "ftp-hope",
  "version": "0.1.2",
  "description": "A ftp client with typescript support under node environment",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "ts:compile": "tsc",
    "ts:watch": "tsc --watch",
    "lint": "eslint --fix --ext .ts . && prettier --check --write .",
    "lint:check": "eslint --ext .ts . && prettier --check .",
    "test": "node tests/index.js",
    "test-listDir": "node tests/listDir.js",
    "test-putFile": "node tests/putFile.js",
    "test-putFolder": "node tests/putFolder.js",
    "test-getFile": "node tests/getFile.js",
    "test-getFolder": "node tests/getFolder.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/ftp-hope.git"
  },
  "keywords": [
    "ftp",
    "node",
    "promise"
  ],
  "dependencies": {
    "@types/ftp": "^0.3.31",
    "ftp": "^0.3.10"
  },
  "author": "Mr.Hope",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "2.1.1",
    "typescript": "4.0.2"
  }
}
